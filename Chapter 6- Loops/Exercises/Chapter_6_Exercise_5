sandwich_orders = ['chicken sandwich', 'pastrami', 'pastrami', 'pastrami', 'egg sandwich', 'seafood sandwich']
finished_sandwiches = []

while sandwich_orders:
    unfinished_orders = sandwich_orders.pop()
    print("I'm working on your " + unfinished_orders)
    finished_sandwiches.append(unfinished_orders)

for i in sandwich_orders:
    print("The following line will print each letter of", i)